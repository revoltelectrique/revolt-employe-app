{
  "expo": {
    "name": "ReVolt Employé",
    "slug": "revolt-employe-app",
    "version": "1.1.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#64191E"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.revoltelectrique.employe",
      "infoPlist": {
        "NSCameraUsageDescription": "Cette application utilise la caméra pour prendre des photos de pièces jointes.",
        "NSPhotoLibraryUsageDescription": "Cette application accède à vos photos pour les pièces jointes.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#64191E"
      },
      "package": "com.revoltelectrique.employe",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "softwareKeyboardLayoutMode": "pan",
      "minSdkVersion": 23,
      "targetSdkVersion": 34
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-notifications"
    ],
    "extra": {
      "eas": {
        "projectId": "fd220489-4272-42e4-968f-c12d9f9e8711"
      }
    },
    "owner": "revolt5403"
  }
}
